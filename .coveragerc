[run]
# Lista exatamente quais pastas de apps devem ser analisadas
source =
    app
    brands
    cart
    categories
    orders
    products

# Lista de padr천es de arquivos a serem omitidos
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    manage.py
    app/settings.py
    app/wsgi.py
    app/asgi.py

[report]
# Mostra as linhas que n찾o foram cobertas
show_missing = True
# No relat처rio do console, n찾o mostra arquivos com 100% de cobertura
skip_covered = False
